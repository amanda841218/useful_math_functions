#check if an integer is a prime number

def find_prime(num):

	number = 3
	
	if num == 1:
		return False
	if num % 2 == 0:
		return False
	
	#find if the num is devisible by other numbers only up to the square root of it
	while num**0.5 >= number:
		if num % number == 0:
			return False
		number += 2
	
	return True
